var pageTracker = null;

function cxTrackEvent(track_url, redirect_url, tracker) {
  if (!tracker) tracker = pageTracker;
  try {
    tracker._trackPageview(track_url);
  } catch(err) {}

  if (redirect_url) {
    setTimeout(function() {window.location = redirect_url;}, 100);
  }
}

function cxTrackItemEvent(track_url, item_id, redirect_url, tracker) {
  if (!tracker) tracker = pageTracker;
  if (item_id) {
    try {
      tracker._setCustomVar(1, "item_id", item_id, 3);
    } catch(err) {}
  }

  cxTrackEvent(track_url, redirect_url, tracker);
}

function cxTrackPageview(tracker) {
  try {
    
      var path = document.location.pathname;
      path = path.replace('idmmhhijggcmbeejedibpdcahpkneegg', 'ext/free/idmmhhijggcmbeejedibpdcahpkneegg/snap_links_lite');
      cxTrackItemEvent(path + document.location.search, 'idmmhhijggcmbeejedibpdcahpkneegg', null, tracker);
    
  } catch(err) {}
}

